import java.util.*;
public class Main{
  public void studentage(){
    Scanner sc = new Scanner(System.in);
    System.out.println("Please Enter the age");
    int age = sc.nextInt();
    System.out.println("Age is "+age);
    }
  public static void main(String args[]){
  Main m1 = new Main();
  m1.studentage();
  }
  }
    
